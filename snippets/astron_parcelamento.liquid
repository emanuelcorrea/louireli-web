            <!--Conteudo - cartao de credito-->
            <div class="modal-parcel-method card-method">

                <ul class="modal-parcel-list">
                    <li class="modal-parcel-list-item">
                        <span>À vista</span>
                        <span>Total: <span id="total-a-vista">{{ price | money }}</span></span>
                    </li>
                    <li class="modal-parcel-list-item">
                        {% liquid
                            assign taxa_segunda_parcela = settings.taxa_segunda_parcela | divided_by: 100 | plus: 1
                        %}
                        <span>2x{% if taxa_segunda_parcela == 1 %} sem juros {% endif %} de <span
                                    id="taxa-segunda-parcela">{{ price | times: taxa_segunda_parcela | divided_by: 2 | money }}</span></span>
                        <span>Total: <span
                                    id="total-segunda-parcela">{{ price | times: taxa_segunda_parcela | money }}</span></span>
                    </li>
                    <li class="modal-parcel-list-item">
                        {% liquid
                            assign taxa_terceira_parcela = settings.taxa_terceira_parcela | divided_by: 100 | plus: 1
                        %}
