                <div class="modal-parcel-cards-wrapper">
                    <div>
                        Aceitamos:
                    </div>
                    <div class="modal-parcel-cards">
                        {% if settings.bandeiras_credit_card %}
                            <img height="35px" src="{{ settings.bandeiras_credit_card | img_url: 'master' }}"
                                 alt="bandeiras de aceitas">
                        {% else %}
                            <img height="35px" src="{{ 'bandeiras-cartoes-credito.png' | asset_url }}"
