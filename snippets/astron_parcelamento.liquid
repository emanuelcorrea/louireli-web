            <!--Conteudo - PIX-->
            <div class="modal-parcel-method pix-method">
                <div class="modal-parcel-informations">
                    <div class="modal-parcel-pricing">
                        {% liquid
                            assign porcentagem = settings.desconto_pix_parcelamento | divided_by: 100
                            assign desconto_pix_parcelamento = 1 | minus: porcentagem
                        %}
                        <div class="modal-parcel-price-cash">
                            <span id="total-pix">R$ {{ price | times: desconto_pix_parcelamento | money_without_currency }}</span>
                        </div>
                        <div class="modal-parcel-price-description">
