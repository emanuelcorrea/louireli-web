{%- style -%}
  .formas-pagto {
    background-color: {{ section.settings.formas-pagto_bg_principal_cor }};
  }
  .section-{{ section.id }}-padding {
    padding-top: {{ section.settings.padding_top | times: 0.75 | round: 0 }}px;
    padding-bottom: {{ section.settings.padding_bottom | times: 0.75 | round: 0 }}px;
  }
  .formas-pagto__title {
    display: flex;
    align-items: center;
    line-height: normal;
    gap: 10px;
    margin-right: 50px;
    min-width: 310px;
  }
  .formas-pagto__title h2 {
    color: {{ section.settings.formas-pagto_txt_principal_cor }};
    font-size: {{ section.settings.formas-pagto_txt_principal_tamanho_desktop }}px;
  }
  .formas-pagto__wrapper {
    display: flex;
    flex-direction: row;
    flex-wrap: nowrap;
    justify-content: flex-start;
    align-items: center;
    padding: 0 50px;
  }
  .formas-pagto__title svg {
    width: {{ section.settings.formas-pagto_principal_size }}px;
    fill: {{ section.settings.formas-pagto_icone_principal_cor }};
  }
  .formas-pagto__blocks {
    display: flex;
    gap: 30px;
    justify-content: flex-start;
    align-items: center;
  }
  .formas-pagto__block {
    width: clamp(40px, 80px, 100%);
  }
  .formas-pagto__block-img {
    object-fit: contain;
    aspect-ratio: 1 / 1;
    max-width: 100%;
  }
  .formas-pagto__block > svg {
    width: 110px;
    max-width: 100%;
    fill: {{ section.settings.formas-pagto_icones_pagto_cor }};
  }


  @media screen and (max-width: 768px) {
    .formas-pagto__wrapper {
      flex-direction: column;
      padding: 0;
    }
    .formas-pagto__title {
      margin-right: 0;
      margin-bottom: 20px;
      justify-content: center;
      max-width: 100%;
      min-width: initial;
      padding: 0 20px;
    }
    .formas-pagto__title h2 {
      font-size: {{ section.settings.formas-pagto_txt_principal_tamanho_mobile }}px;
      text-align: center;
    }
    .formas-pagto__blocks {
      justify-content: center;
      flex-wrap: wrap;
    }
    .formas-pagto__block {
      width: clamp(40px, 40px, 100%);
    }
  }


{%- endstyle -%}
